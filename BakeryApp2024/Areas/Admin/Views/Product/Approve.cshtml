@model IEnumerable<ProductServiceModel>

@{
    ViewBag.Title = "Approve";
}
<h2 class="text-center">@ViewBag.Title</h2>

<hr />
<h4>Products to approve</h4>
@if (!Model.Any())
{
    <h2 class="text-center">You have no products to approve!</h2>
}
<div class="row">
    @foreach (var product in Model)
    {
        @await Html.PartialAsync("~/Areas/Admin/Views/Shared/_ApproveProductPartial.cshtml", product)
    }
</div>